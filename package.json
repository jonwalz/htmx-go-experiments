{
  "name": "project",
  "version": "0.0.0",
  "description": "Frontend part of the Gowebly project.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "PARCEL_WORKER_BACKEND=process parcel build --log-level=verbose ./assets/scripts.js ./assets/styles.scss --dist-dir ./static",
    "dev": "PARCEL_WORKERS=0 parcel build --log-level=verbose ./assets/scripts.js ./assets/styles.scss --dist-dir ./static --no-optimize",
    "watch": "PARCEL_WORKERS=0 parcel watch ./assets/scripts.js ./assets/styles.scss --dist-dir ./static",
    "build:bun": "bun build ./assets/scripts.js ./assets/styles.scss --outdir ./tmp --minify",
    "bun:watch": "bun build ./assets/scripts.js ./assets/styles.scss --outdir ./static --minify --watch",
    "bundev": "bun run ./ibun.js --watch"
  },
  "dependencies": {
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "daisyui": "latest",
    "htmx.org": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "@types/bun": "^1.1.6",
    "autoprefixer": "latest",
    "parcel": "latest",
    "postcss": "latest",
    "prettier": "latest"
  }
}
